/*
*/

process {

    withLabel:process_low {
        memory = '2G'
        cpus = 1
    }

  withLabel:process_medium {
        memory = '4G'
        cpus = 4
    }

    withLabel:process_high {
        memory = '8G'
        cpus = 6
    }

withLabel:low_cpu_low_mem {
        memory = '1G'
        cpus = 1
    }

withLabel:med_cpu_med_mem {
        memory = '4G'
        cpus = 2
    }

withLabel:med_cpu_high_mem {
        memory = '8G'
        cpus = 4
    }

withLabel:high_cpu_high_mem {
        memory = '8G'
        cpus = 8
    }

withLabel:low_cpu_high_mem {
        memory = '8G'
        cpus = 1
    }
}
